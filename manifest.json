{
    "manifest_version": 2,
    "name":    "work",
    "description": "autofill form",
    "version": "0.0",

    "background": {
        "persistent": false,
        "scripts": [
          "util.js",
          "background.js"
        ]
    },
    "content_scripts": [
      {
        "matches": ["*://login.taobao.com/*"],
        "js":      [
          "autofill.js"
        ],
        "run_at":  "document_end"
      },
      {
        "matches": ["*://mai.taobao.com/*"],
        "js":      [
          "autoredirect.js"
        ],
        "run_at":  "document_start"
      },
      {
        "matches": ["*://qudao.gongxiao.tmall.com/*"],
        "js":      [
          "jquery/jquery-1.11.3.min.js",
          "util.js",
          "autopaging.js"
        ],
        "run_at":  "document_end"
      }
    ],
    "browser_action": {
        "default_title": "Test Extension"
    },

    "permissions": [
      "tabs",
      "activeTab",
      "downloads",
      "<all_urls>",
      "https://*/*",
      "http://*/*"
    ]
}
