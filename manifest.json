{
    "manifest_version": 2,
    "name":    "work",
    "description": "autofill form",
    "version": "0.0",

    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js":      ["inject.js"],
        "run_at":  "document_end"
      },
      {
        "matches": ["*://login.taobao.com/*"],
        "js":      ["autofill.js"],
        "run_at":  "document_end"
      },
      {
        "matches": ["*://mai.taobao.com/*"],
        "js":      ["autoredirect.js"],
        "run_at":  "document_start"
      },
      {
        "matches": ["*://qudao.gongxiao.tmall.com/*"],
        "js":      ["autopaging.js"],
        "run_at":  "document_end"
      }
    ],
    "browser_action": {
        "default_title": "Test Extension"
    },

    "permissions": ["tabs","activeTab","<all_urls>"]
}
